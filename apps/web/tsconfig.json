{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "types": ["vitest/globals", "@testing-library/jest-dom", "@cloudflare/workers-types"]
  }
}